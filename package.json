    "fs-extra": "^8.1.0",
    "github-syntax-dark": "^0.5.0",
    "jquery": "^3.4.1",
    "request": "^2.88.0",
    "semver": "^6.3.0",
    "sha1": "^1.1.1",
    "tar-fs": "^2.0.0",
